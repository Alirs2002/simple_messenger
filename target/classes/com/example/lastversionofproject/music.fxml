<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="190.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lastversionofproject.musiccontroller">
   <children>
      <ImageView fitHeight="190.0" fitWidth="620.0" pickOnBounds="true">
         <image>
            <Image url="@G6ry43.jpg" />
         </image>
      </ImageView>
      <Label fx:id="label" alignment="CENTER" layoutX="-1.0" layoutY="-1.0" prefHeight="86.0" prefWidth="621.0" text="MP3 Player">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <ProgressBar fx:id="progressbar" layoutX="-1.0" layoutY="86.0" prefHeight="18.0" prefWidth="621.0" progress="0.0" />
      <Button fx:id="previousbutton" layoutX="402.0" layoutY="124.0" mnemonicParsing="false" onAction="#previousmedia" text="Previous">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="playbutton" layoutX="119.0" layoutY="124.0" mnemonicParsing="false" onAction="#playmedia" text="Play">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="pausebutton" layoutX="201.0" layoutY="124.0" mnemonicParsing="false" onAction="#pausemedia" text="Pause">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetbutton" layoutX="310.0" layoutY="125.0" mnemonicParsing="false" onAction="#resetmedia" text="Reset">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="nextbutton" layoutX="514.0" layoutY="125.0" mnemonicParsing="false" onAction="#nextmedia" text="Next">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Slider fx:id="volumeslider" layoutX="593.0" layoutY="112.0" orientation="VERTICAL" prefHeight="64.0" prefWidth="14.0" />
      <Button fx:id="backbutton" layoutX="33.0" layoutY="124.0" mnemonicParsing="false" onAction="#goback" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
